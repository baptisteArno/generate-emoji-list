{
	"compilerOptions": {
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"lib": ["ESNext"],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "dist",
		"skipLibCheck": true,
		"strict": true,
		"target": "es5"
	}
}
